# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Google OAuth Configuration
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Redirect URI - should match what you configure in Google Cloud Console
NEXT_PUBLIC_GOOGLE_REDIRECT_URI=/api/auth/google/callback

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://shikshanamgithub.vercel.app
NEXT_PUBLIC_API_BASE_URL=https://shikshanamgithub.vercel.app/api

# Environment
NODE_ENV=production
