# Supabase Environment Variables
# Copy these to your shell environment or .env.local file

# Your Supabase project URL
export SUPABASE_URL=https://krkmeeivttldjtzcuyue.supabase.co

# Service role key (required for uploads and admin operations)
# Get this from: Supabase Dashboard > Settings > API > service_role key
export SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Anonymous key (required for public downloads)
# Get this from: Supabase Dashboard > Settings > API > anon public key
export SUPABASE_ANON_KEY=your_anon_key_here

# Optional: Database password (if needed for direct connections)
export SUPABASE_DB_PASSWORD=your_db_password_here

# To use these variables, run:
# source supabase-env.example
# (after filling in your actual values)

# GitHub API Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_OWNER=your_github_owner_here
GITHUB_REPO=your_github_repo_here
